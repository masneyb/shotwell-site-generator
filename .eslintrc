env:
  es2021: true
  browser: true
extends:
  - airbnb-base
plugins:
  - eslint-plugin-html
parserOptions:
  ecmaVersion: 12
rules: {
  "arrow-body-style": 0,
  "class-methods-use-this": 0,
  "func-names": 0,
  "max-classes-per-file": 0,
  "max-len": 0,
  "no-continue": 0,
  "no-multi-assign": 0,
  "no-param-reassign": 0,
  "no-restricted-syntax": 0,
  "no-undef": 0,
  "no-unused-vars": 0,
  "prefer-destructuring": 0
}

